	"scripts": {
		"build": "npm run clean && npm run build:dev && npm run check:dependencies",
		"build:copy": "shx mkdir dist && shx cp ./src/index.tmpl.html ./src/favicon.ico ./dist",
		"build:dev": "npm run build:messages && npm run build:lint && npm test && npm run build:dist",
		"build:dist": "npm run build:messages && npm run build:readme && npm run build:copy && webpack --progress --env=dist",
		"build:lint": "stylelint \"**/*.scss\" --syntax scss && eslint --ext .js,.jsx .",
		"build:lint:fix": "stylelint \"**/*.scss\" --syntax scss --fix && eslint --fix --ext .js,.jsx .",
		"build:readme": "node-readme",
		"build:messages": "generate-messages ./src > ./src/messages/en.json",
		"clean": "shx rm -rf README.md src/messages/*.json dist coverage",
		"test": "karma start",
		"test:nocoverage": "env NODE_ENV=test-no-coverage npm run test",
		"test:watch:coverage": "npm run test -- --autoWatch=true --singleRun=false",
		"test:watch": "npm run test:nocoverage -- -- --autoWatch=true --singleRun=false",
		"test:chrome": "npm run test:nocoverage -- -- --browsers Chrome",
		"test:chrome:watch": "npm run test:watch -- --browsers Chrome",
		"check:dependencies": "npm run check:used && npm run check:unused",
		"check:used": "dependency-check ./package.json",
		"check:unused": "dependency-check ./package.json --unused --no-dev"
	},
	"gitHead": "25f8a7793098bc37cb881771adf33995217555f5"
