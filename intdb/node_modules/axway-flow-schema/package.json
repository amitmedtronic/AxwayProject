{
	"name": "axway-flow-schema",
	"version": "5.4.3",
	"description": "Axway flow JSON schema",
	"homepage": "https://platform.axway.com",
	"author": {
		"name": "Axway",
		"email": "support@axway.com",
		"url": "https://axway.com"
	},
	"license": "SEE LICENCE IN LICENSE",
	"keywords": [
		"amplify",
		"api-builder",
		"axway",
		"@axway/flow",
		"schema"
	],
	"engines": {
		"node": ">=8.9"
	},
	"main": "src/index.js",
	"files": [
		"src",
		"flow*.json",
		"LICENSE",
		"README.md"
	],
	"devDependencies": {
		"ajv": "^6.12.5"
	},
	"repository": {
		"type": "git",
		"url": "https://git.ecd.axway.int/amplify/api-builder"
	},
	"scripts": {
		"build": "npm run clean && npm run build:dev",
		"build:dev": "npm run build:lint && npm run test && npm run build:readme",
		"build:lint": "eslint .",
		"build:readme": "node-readme",
		"clean": "shx rm -rf coverage",
		"test": "nyc mocha ./test",
		"test:watch": "npm test -- --watch",
		"check:dependencies": "npm run check:used && npm run check:unused",
		"check:used": "dependency-check ./package.json",
		"check:unused": "dependency-check ./package.json --unused --no-dev -i deep-equal"
	},
	"gitHead": "bea449db0c25c6708e1d1f4e499b6025422bfbb3"
}
