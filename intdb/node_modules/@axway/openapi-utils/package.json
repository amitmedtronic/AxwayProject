{
	"name": "@axway/openapi-utils",
	"version": "1.2.10",
	"description": "OpenAPI/Swagger utilities for API Builder",
	"main": "src/index.js",
	"engines": {
		"node": ">=8.9"
	},
	"scripts": {
		"build": "npm run clean && npm run build:dev && npm run check:dependencies",
		"build:dev": "npm run build:lint && npm run test",
		"build:lint": "eslint .",
		"clean": "shx rm -rf coverage",
		"test": "nyc mocha ./test",
		"test:fix": "eslint . --fix && npm run test",
		"test:watch": "npm test -- --watch",
		"check:dependencies": "npm run check:used && npm run check:unused",
		"check:used": "dependency-check ./package.json",
		"check:unused": "dependency-check ./package.json --unused --no-dev -i deep-equal"
	},
	"repository": {
		"type": "git",
		"url": "https://git.ecd.axway.int/amplify/api-builder"
	},
	"keywords": [
		"axway",
		"amplify",
		"api-builder",
		"openapi",
		"swagger",
		"api"
	],
	"author": {
		"name": "Axway",
		"email": "support@axway.com",
		"url": "https://axway.com"
	},
	"license": "SEE LICENCE IN LICENSE",
	"dependencies": {
		"@apidevtools/swagger-parser": "10.0.3",
		"@axway/json-refs": "^4.0.0",
		"@axway/swagger-tools": "^0.11.2",
		"axway-schema": "4.3.1"
	},
	"devDependencies": {
		"@axway/api-builder-test-utils": "1.5.8"
	},
	"files": [
		"src",
		"README.md",
		"LICENSE"
	],
	"nyc": {
		"per-file": false,
		"statements": 95,
		"branches": 85,
		"lines": 86
	},
	"gitHead": "5613eb3868fe0fe1c861f0530dc0f6a1af95e85a"
}
