{
	"name": "@axway/api-builder-sdk",
	"version": "1.2.2",
	"description": "SDK for implementing custom plugins for API Builder",
	"homepage": "https://platform.axway.com",
	"author": {
		"name": "Axway",
		"email": "support@axway.com",
		"url": "https://axway.com"
	},
	"license": "SEE LICENCE IN LICENSE",
	"keywords": [
		"axway",
		"amplify",
		"api-builder",
		"sdk"
	],
	"engines": {
		"node": ">= 8.9",
		"apibuilder": ">=4.5.0"
	},
	"main": "src/index.js",
