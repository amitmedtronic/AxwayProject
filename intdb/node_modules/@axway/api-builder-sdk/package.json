		"docs",
		"README.md",
		"LICENSE",
		"api-builder-sdk-schema-v1.yaml"
	],
	"dependencies": {
		"@axway/api-builder-uri-utils": "1.0.0",
		"ajv": "^6.12.5",
		"js-yaml": "^3.13.1"
	},
	"devDependencies": {
		"@axway/api-builder-test-utils": "1.5.8",
		"@axway/flow": "6.7.16"
	},
	"repository": {
		"type": "git",
		"url": "https://git.ecd.axway.int/amplify/api-builder"
	},
	"nyc": {
		"all": false
	},
	"scripts": {
		"build": "npm run build:lint && npm run build:docs && npm run test",
		"build:lint": "eslint .",
		"build:lint:fix": "eslint . --fix",
		"build:docs": "jsdoc2md src/index.js > docs/API.md",
		"test": "nyc mocha ./test",
		"test:debug": "mocha --inspect-brk ./test",
		"test:watch": "npm test -- --watch",
		"test:nocoverage": "mocha ./test"
	},
