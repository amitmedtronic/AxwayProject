		"schema",
		"LICENSE",
		"README.md"
	],
	"dependencies": {
		"@axway/api-builder-config": "4.0.2",
		"@axway/api-builder-uri-utils": "1.0.0",
		"@axway/openapi-utils": "1.2.10",
		"@axway/swagger-tools": "^0.11.2",
